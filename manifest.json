{
  "manifest_version": 2,

  "name": "Godwinner",
  "description": "Detect inappropriate comparisons to Hitler",
  "version": "0.1",

  "browser_action": {
   "default_icon": "Godwin.png",
   "default_popup": "popup.html"
   },

  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-2.2.3.min.js", "content.js"]
  }
  ],

  "permissions": [
    "activeTab"
   ]
}